/*============================================*/
/*frame_menu*/
#frame_bmenu, #frame_smenu
{
    border: 1px solid #FFEBCD;
    background-color: rgba(234,76,60, 180);
}
/*============================================*/
/*frame_head*/
#frame_head
{
    border: 1px solid #D2B48C;
    border-radius: 5px;
    background-color: rgba(116,115,153, 100);

}
#frame_head #btn_head_menu
{
    border-radius: 10px;
}

#frame_head #btn_head_menu:pressed
{
		padding-left: 5px;
		padding-top: 5px;
}

#frame_head #btn_head_search
{
    border-radius: 5px;
    background-color: #FFEFD5;
}
#frame_head #btn_head_search:hover
{
        border-radius: 10px;
	    margin: 2px 2px;
}
#frame_head #btn_head_search:pressed
{
		padding-left: 5px;
		padding-top: 5px;
}
/*============================================*/
/*menu_bbtn*/
#frame_bmenu QPushButton
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0.505682, x2:1, y2:0.477, stop:0 rgba(168,202,186, 0), stop:1 rgba(93,65,87, 140));
	border: 0;
	margin-bottom: 15px;
	border-top-left-radius: 15px;
	border-bottom-right-radius: 15px;


}
#frame_bmenu QPushButton:hover
{
		background-color: rgba(227,253,245, 255);
}
#frame_bmenu QPushButton:pressed
{
		padding-left: 10px;
		padding-top: 5px;
}

