/*============================================*/
/*frame_folder_head_btn*/
#frame_folder_head QPushButton {
	margin-bottom: 5px;
	border-radius: 5px;
    background-color: qlineargradient(spread:pad, x1:0, y1:0.505682, x2:1, y2:0.477, stop:0 rgba(132,250,176,50), stop:1 rgba(143,211,244,120));
    border: 1px solid #2d8b4e; /* 边框颜色 */
    color: #bdaead; /* 文本颜色 */
    font-size: 14px; /* 字体大小 */
    font-weight: bold; /* 字体粗细 */
}
#frame_folder_head QPushButton:hover {
    background-color: #FF6666; /* 鼠标悬停时的背景色 */
}
#frame_folder_head QPushButton:pressed {
    padding-left: 5px;
	padding-top: 5px;
    border: 1px solid #f25a47; /* 按下时的边框颜色 */
}
#frame_folder_head QPushButton:focus {
    outline: none; /* 焦点时去除虚线框 */
}
#frame_folder_head QPushButton:disabled {
    background-color: #cccccc; /* 禁用时的背景色 */
    border: 1px solid #999999; /* 禁用时的边框颜色 */
    color: #666666; /* 禁用时的文本颜色 */
}
#frame_folder_head QPushButton#specialButton {
    background-color: #e74c3c; /* 特殊按钮的背景色 */
    border: 1px solid #c0392b; /* 特殊按钮的边框颜色 */
    color: #ffffff; /* 特殊按钮的文本颜色 */
}
