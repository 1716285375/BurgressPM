/*tableWidget_folder*/
#tableWidget_folder {
    background-color: rgb(252, 252, 252); /* 淡白色背景 */
    alternate-background-color: #d4d4d4; /* 交替行背景 */
    selection-background-color: #954747; /* 选中行的橘色背景 */
    gridline-color: #c3c3c3; /* 网格线颜色 */
}
#tableWidget_folder QHeaderView::section {
    background-color: rgba(147,181,207, 220); /* 景蓝表头背景 */
    color: #243264; /* 白色表头文字 */
    text-align: center;
    font-size: 16px;
    font-family: 'Oleo Script';
    font: bold 12px;
}
#tableWidget_folder::item {
    border-bottom: 1px solid #c3c3c3; /* 单元格底边框 */
    color: #222222; /* 单元格文字颜色 */
	text-align: center; /* 居中显示文本 */
}

#tableWidget_folder::item:selected {
    background-color: #f7cdbc; /* 选中单元格的暗红色背景 */
    color: #f9f1db; /* 选中单元格的白色文字 */
}

/*listWidget_folder*/
#listWidget_folder{
    background-color: #f7f7f7; /* 浅灰色背景 */
}
#listWidget_folder::item {
    height: 30px; /* 设置列表项高度 */
    font-family: '鸿雷拙书简体', 'sans-serif';
    font-size: 14px; /* 设置字体大小 */
    background-color: #ffffff; /* 白色列表项背景 */
    border: 1px solid #d1d1d1; /* 列表项边框 */
    color: #333333; /* 列表项文字颜色 */
}

#listWidget_folder::item:selected {
    background-color: #a0c4ff; /* 选中列表项的淡蓝色背景 */
    color: #ffffff; /* 选中列表项的白色文字 */
    border: none;
    outline: none;
}
#listWidget_folder::item:focus {
    border: none;
    outline: none;
}
/*END: page_folder*/
